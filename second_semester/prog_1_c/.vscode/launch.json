{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Anexar",
      "type": "cppdbg",
      "request": "attach",
      "program": "insira o nome do programa, por exemplo ${workspaceFolder}/a.exe",
      "processId": "${command:pickProcess}",
      "MIMode": "gdb",
      "miDebuggerPath": "/path/to/gdb",
      "setupCommands": [
          {
              "description": "Habilitar a reformatação automática para gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        ""
      ],
      "stopAtEntry": false,
      "cwd": "c:/Users/2212120017/Documents/repos/graduation_projects/second_semester/prog_1_c",
      "environment": [],
      "program": "c:/Users/2212120017/Documents/repos/graduation_projects/second_semester/prog_1_c/build/Debug/outDebug",
      "internalConsoleOptions": "openOnSessionStart",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}